body#members
{
  .member-area
  {
    .member-listing
    {
      a
      {
        color: #135EA3;
      }
    }

    form
    {
      .clearfix
      {
        margin-bottom: 0;
      }
    }
    
    .excel-section
    {
      .excel-heading
      {
        img
        {
          position: relative;
          top: 0;
          margin: 0;
        }
      }
    }
  }
  
  div#wrapper h1
  {
    margin: 0;
    float: left;
  }
  
  button.remove
  {
    float: none;
  }
  
  .form-body input.ccn {
    width:220px;
  }
  .form-body input.ccv {
    width:90px;
  }

  .form-body select.mns {
    width:105px;
  }
  .form-body select.yrs {
    width:90px;
  }
  .form-body input.none {
    width:auto; height:auto;
    background:none;
    border:none;
  }
  .form-body .signup-bttn {
    background-color:#C5C3C3;
    border:1px solid #8E8E8E;
    line-height:27px;
    padding:0 18px;
    font-size:12px;
    text-shadow:1px 1px 1px #fff;
    margin-right:12px;
    float:none;
  }
  .form-body .signup-bttn:hover {
    color:#000;
  }
  
  .form-body label {
    display:block;
    font-weight:bold;
    font-size:14px;
    padding:0 0 5px;
    text-align:left;
    width:100%;
  }
  .form-body small {
    font-size:12px;
    color:#646464;
  }
  .form-body label.left {
    float:left;
    width:120px;
    line-height:29px;
  }
  .payinfo {
    width:275px;
    float:left;
  }
  .form-body select {
    border:1px solid #DCDCDC;
    background-repeat: repeat-x;
    background-image: -khtml-gradient(linear, left top, left bottom, from(#F2F2F2), to(#F2F2F2));
    background-image: -moz-linear-gradient(top, #fff, #F2F2F2);
    background-image: -ms-linear-gradient(top, #fff, #F2F2F2);
    background-image: -webkit-gradient(linear, left top, left bottom, color-stop(100%, #F2F2F2), color-stop(0%, #F2F2F2));
    background-image: -webkit-linear-gradient(top, #F2F2F2, #F2F2F2);
    background-image: -o-linear-gradient(top, #F2F2F2, #F2F2F2);
    background-image: linear-gradient(top, #F2F2F2, #F2F2F2);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#F2F2F2', endColorstr='#F2F2F2', GradientType=0);
    font-size:12px; font-weight:bold; color:#B3B3B3;
    height:auto;
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    border-radius: 0;
    margin-bottom:10px;
    color:#646464;
    font-size:12px;
    font-family:'Open Sans', Arial, Helvetica, sans-serif;
    padding:5px;
  }

  /*Excel Section*/
  .excel-section {
    padding: 7px 46px 15px 0;
    height:100%;
    overflow:hidden;
  }
  .excel-heading {
      background:url(<%= asset_path 'v2/excel-icon.gif' %>) 0 50% no-repeat;
    padding:8px 0 8px 30px;
    font-size:14px;
    font-weight:bold;
    color:#000;
    position:relative;
  }
  .excel-heading img {
    top:10px;
    margin-left:5px;
  }
  .excel-heading a {
    color:#135EA3;
    text-decoration:none;
    font-size:12px;
    margin-right:8px;
    padding-right:15px;
  }
  .excel-heading a:hover {
    color:#0B3760;
  }
  .excel-header {
    background-color:#F6F6F6;
    border:1px solid #B8B8B8;
    height:100%;
    overflow:hidden;
    font:bold 12px/16px Arial, Helvetica, sans-serif;
    color:#3E3E3E;
  }
  .excel-header div {
    float:left;
    border-right:1px solid #B8B8B8;
    padding:10px;
    height:20px;
    vertical-align:middle;
  }
  .excel-name {
    width:141px;
    padding-top:18px;
  }
  .excel-email {
    width:130px;
    padding-top:18px;
  }
  .excel-perm {
    width:130px;
    padding-top:12px;
    padding-bottom:16px;
    line-height:12px;
  }
  .excel-role {
    width:148px;
    padding-top:12px;
    padding-bottom:16px;
    line-height:12px;
  }
  .excel-blank {
    width:75px;
    padding-top:18px;
  }
  textarea.area {
    border:1px solid #B8B8B8;
    border-top:none;
    padding:10px;
    width:858px;
    font:normal 12px/20px Arial, Helvetica, sans-serif;
    color:#9A9A9A;
    overflow:auto;
    min-height:80px;
  }
  .excel-section .import {
    background-color:#C5C3C3;
    border:1px solid #8E8E8E;
    line-height:27px;
    padding:0 18px;
    font-size:12px;
    text-shadow:1px 1px 1px #fff;
    margin-right:12px;
    margin-top:12px;
  }
  .excel-section .import:hover {
    color:#000;
  }
  
  .member-perm select {
    width:120px;
    height:29px;
  }
  
  .member-role select {
    width:136px;
    height:29px;
  }
  
  .paym-info-bar
  {
    span.member-count
    {
      padding: 0;
    }    
  }
}